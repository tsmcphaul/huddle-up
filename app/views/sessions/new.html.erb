<h1>Log In Here</h1>

<% if @user.errors.any?%>
<ol>
    <% @user.errors.full_messages.each do |message|%>
    <li><%= message%> </li>
    <% end %>
</ol>
<% end %>

<%= form_for :user, url: login_path do |f|%>
<%= f.label :email %>
<%= f.text_field :email %>

<%= f.label :password %>
<%= f.password_field :password %>

<%= f.submit "Log In" %>

<% end %>