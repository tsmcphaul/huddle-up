<% if @project.errors.any?%>
<ol>
    <% @project.errors.full_messages.each do |message|%>
    <li><%= message%> </li>
    <% end %>
</ol>
<% end %>

<%= form_for @project do |f|%>
    <%= f.label :name%>
    <%= f.text_field :name%>
    <br>
    <br>
    <%= f.submit "Submit", class:"btn btn-primary"%>
    
<% end %>

